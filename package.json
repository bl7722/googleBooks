{
  "name": "google-books",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "cd google-books && if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd google-books && node server.js",
    "start:dev": "cd google-books && concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd google-books && cd client && npm run start",
    "seed": "cd google-books && node scripts/seedDB.js",
    "install": "cd google-books && cd client && npm install",
    "build": "cd google-books && cd client && npm run build",
    "heroku-postbuild": "cd google-books && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "react-router-dom": "^5.2.0",
    "seed": "^0.4.4"
  }
}
